{
  "name": "vyer-front-end-coding-challenge",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run relay && parcel build src/index.html",
    "clean": "rm -rf dist",
    "lint:fix": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "relay": "relay-compiler $@",
    "start": "npm run relay && parcel src/index.html",
    "test": "jest"
  },
  "relay": {
    "src": "./src/",
    "schema": "./schema.docs.graphql",
    "language": "typescript"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-relay": "^13.0.1",
    "relay-runtime": "^13.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-relay": "^13.0.0",
    "@types/relay-runtime": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-jest": "^27.4.6",
    "babel-plugin-relay": "^13.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.4.7",
    "parcel": "^2.2.1",
    "prettier": "latest",
    "relay-compiler": "^13.0.1",
    "typescript": "^4.5.4"
  }
}
